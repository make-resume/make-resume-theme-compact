{{#if resume.interests}}
{{#>section title=(sectionTitle resume "interests" "Interests")}}
	<div class="columns is-multiline is-gapless m-0">
		{{#each resume.interests}}
		<div class="column is-3 item">
			<h5 class="skill__title">{{this.name}}</h5>
			{{#if this.keywords}}
			<div class="item__content">
				<ul>
					{{#each this.keywords}}
					<li>{{this}}</li>
					{{/each}}
				</ul>
			</div>
			{{/if}}
		</div>
		{{/each}}
	</div>
{{/section}}
{{/if}}